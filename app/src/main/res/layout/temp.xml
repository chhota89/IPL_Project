<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/constraintLayout2">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        tools:ignore="HardcodedText"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout2"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/editText7" />

    <ImageView
        android:src="@android:drawable/ic_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        tools:ignore="ContentDescription"
        app:layout_constraintLeft_toRightOf="@+id/textView6"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@+id/editText7" />

    <EditText
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:inputType="textEmailAddress"
        android:ems="10"
        android:id="@+id/editText7"
        app:layout_constraintLeft_toRightOf="@+id/imageView3"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout2"
        android:layout_marginTop="56dp"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout2"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintHorizontal_bias="1.0" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        tools:ignore="HardcodedText"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout2"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        android:layout_marginTop="40dp"
        app:layout_constraintRight_toRightOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="0.93" />

    <ImageView
        android:src="@android:drawable/ic_lock_idle_alarm"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:id="@+id/imageView4"
        tools:ignore="ContentDescription"
        app:layout_constraintLeft_toRightOf="@+id/textView7"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintRight_toRightOf="@+id/imageView3"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        tools:layout_constraintBottom_creator="1" />

    <EditText
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/editText8"
        app:layout_constraintLeft_toRightOf="@+id/imageView4"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        android:layout_marginTop="24dp"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout2"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        tools:layout_constraintRight_creator="1" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:weightSum="2"
        android:id="@+id/linearLayout"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout2"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout2">

        <Button
            android:text="Button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/button6"
            android:layout_weight="1"
            tools:ignore="ButtonStyle,HardcodedText"
            app:layout_constraintLeft_toLeftOf="@+id/linearLayout"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintRight_toLeftOf="@+id/button5"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintBaseline_toBaselineOf="@+id/button5"
            tools:layout_constraintBaseline_creator="1" />

        <Button
            android:text="Button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:id="@+id/button5"
            tools:ignore="ButtonStyle,HardcodedText"
            app:layout_constraintTop_toTopOf="@+id/linearLayout"
            tools:layout_constraintTop_creator="1"
            app:layout_constraintRight_toRightOf="@+id/linearLayout"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/linearLayout"
            tools:layout_constraintBottom_creator="1" />
    </LinearLayout>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="20dp"
        app:relativeBegin="20dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="20dp"
        app:relativeBegin="20dp" />

</android.support.constraint.ConstraintLayout>